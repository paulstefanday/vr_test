{
  "name": "aframe-hello-world-boilerplate",
  "description": "Boilerplate with A-Frame's 'Hello, World!' of WebVR.",
  "version": "0.3.0",
  "license": "MIT",
  "scripts": {
    "start": "budo index.js --live --verbose --port 3000 --open -- -g yo-yoify",
    "deploy": "ghpages",
    "ghpages": "ghpages",
    "build" : "browserify index.js -g yo-yoify -o bundle.js -g es2040"
  },
  "devDependencies": {
    "budo": "^7.0.0",
    "ghpages": "0.0.3"
  },
  "keywords": [
    "aframe",
    "aframe-example",
    "aframe-boilerplate",
    "aframe-scene",
    "webvr",
    "vr"
  ],
  "dependencies": {
    "aframe-animation-component": "^3.0.4",
    "aframe-text-component": "^0.3.0",
    "browserify": "^13.1.1",
    "es2040": "^1.2.4",
    "yo-yo": "^1.3.1",
    "yo-yoify": "^3.4.1"
  }
}
